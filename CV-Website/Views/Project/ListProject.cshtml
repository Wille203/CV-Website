@if (User.Identity.IsAuthenticated)
{
    <td><a class="btn btn-info btn-sm" asp-controller="Project" asp-action="CreateProject">Skapa ett projekt</a></td>
}


<table class="table table-striped table-bordered table-hover">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Titel</th>
            <th scope="col">Beskrivning</th>
            <th scope="col">Mer Info</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var project in Model)
        {
            <tr>
                <td>@project.Title</td>
                <td>@project.Description</td>
                <td>
                    <a class="btn btn-info btn-sm" asp-controller="Project" asp-action="ProjectPage" asp-route-id="@project.ProjectId">Mer Info</a>
                </td>
            </tr>
        }
    </tbody>
</table>